<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600" prefWidth="400" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testjavafx.ChatController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <GridPane maxHeight="816.0" maxWidth="533.0" prefHeight="816.0" prefWidth="463.0">
     <columnConstraints>
       <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" percentWidth="25.0" />
       <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
         <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
         <ColumnConstraints hgrow="SOMETIMES" percentWidth="25.0" />
     </columnConstraints>
     <rowConstraints>
         <RowConstraints maxHeight="643.0" minHeight="10.0" prefHeight="567.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" percentHeight="8.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" percentHeight="8.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <TextArea fx:id="ta_chat" maxWidth="1.7976931348623157E308" prefHeight="194.0" prefWidth="400.0" GridPane.columnSpan="4">
            <font>
               <Font size="14.0" />
            </font>
            <GridPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin></TextArea>
         <TextField fx:id="tf_message" onAction="#sendMessage" prefHeight="25.0" prefWidth="300.0" promptText="message" GridPane.columnSpan="3" GridPane.rowIndex="1">
            <font>
               <Font size="18.0" />
            </font>
            <GridPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin></TextField>
         <Button mnemonicParsing="false" onAction="#sendMessage" prefHeight="36.0" prefWidth="100.0" text="send!" GridPane.columnIndex="3" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin></Button>
         <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Button" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <GridPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin></Button>
         <Button maxHeight="1.7976931348623157E308" maxWidth="-Infinity" mnemonicParsing="false" prefHeight="47.0" prefWidth="190.0" text="Back" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <GridPane.margin>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin></Button>
      </children>
      <VBox.margin>
         <Insets bottom="-10.0" left="-10.0" right="-10.0" top="-10.0" />
      </VBox.margin>
   </GridPane>
</VBox>
